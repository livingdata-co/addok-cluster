{
  "name": "addok-cluster",
  "version": "0.9.0",
  "description": "Clustering addok geocoding capabilities from your Node.js process",
  "main": "index.js",
  "type": "module",
  "repository": "https://github.com/livingdata-co/addok-cluster",
  "author": "Jérôme Desboeufs <jerome@livingdata.co>",
  "license": "MIT",
  "private": false,
  "files": [
    "lib",
    "index.js"
  ],
  "scripts": {
    "lint": "xo",
    "test": "c8 --all ava",
    "test-lcov": "c8 --reporter=lcov --reporter=text --all ava"
  },
  "dependencies": {
    "debug": "^4.4.3",
    "execa": "^8.0.1",
    "http-errors": "^2.0.0",
    "ioredis": "^5.8.1",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.1.6",
    "python-shell": "^5.0.0",
    "supports-color": "^10.2.2"
  },
  "devDependencies": {
    "ava": "^6.4.1",
    "c8": "^10.1.3",
    "tmp": "^0.2.5",
    "xo": "^0.60.0"
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "comma-dangle": [
        "error",
        "never"
      ],
      "promise/prefer-await-to-then": "off",
      "unicorn/prevent-abbreviations": "off",
      "unicorn/prefer-event-target": "off",
      "import/order": [
        "error",
        {
          "newlines-between": "ignore"
        }
      ]
    }
  },
  "c8": {
    "include": [
      "lib/*.js"
    ]
  },
  "engines": {
    "node": ">= 20.9"
  }
}
